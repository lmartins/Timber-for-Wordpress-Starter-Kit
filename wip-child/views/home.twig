{% extends "base.twig" %}

{% block banner %}
    {% for banner in banners_top %}
        {% include ['partials/banner-'~post.post_type~'.twig', 'partials/banner.twig'] %}
    {% endfor %}
{% endblock %}


{% block content %}

    {% for post in posts %}
        {% do action('tha_entry_before') %}
        {% include ['tease-'~post.post_type~'.twig', 'tease.twig'] %}
        {% do action('tha_entry_after') %}
    {% endfor %}

{% endblock %}